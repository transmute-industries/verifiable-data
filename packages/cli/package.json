  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "main.js"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "pretransmute": "npm run build",
    "transmute": "node main.js",
